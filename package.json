  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV=production rollup -c",
    "build:storybook": "storybook build",
    "changeset": "changeset",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "prepare": "husky install",
    "release": "changeset publish",
    "storybook": "storybook dev -p 6006 --quiet",
    "storybook:build": "storybook build && touch ./storybook-static/.nojekyll",
    "storybook:upgrade": "npx sb@latest upgrade",
    "storybook:deploy": "yarn storybook:build && touch ./storybook-static/.nojekyll && gh-pages -d ./storybook-static -t true",
    "test": "NODE_ENV=test jest",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watch",
    "docs": "npx typedoc --options typedoc.json",
    "commit": "git-cz"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,mdx,yml,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "not dead",
    "not edge <= 18",
    "not ie 11",
    "not op_mini all"
  ],
