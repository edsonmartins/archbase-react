{
  "name": "@archbase/feature-flags",
  "version": "3.0.15",
  "description": "Feature flags integration for Archbase React v3 using Unleash",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly || true",
    "dev": "vite build --watch",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@mantine/core": "8.3.12",
    "react": "^18.3.0 || ^19.2.0",
    "react-dom": "^18.3.0 || ^19.2.0"
  },
  "dependencies": {
    "@archbase/core": "workspace:*",
    "@unleash/proxy-client-react": "^3.6.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "typescript": "^5.8.0",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^4.5.0"
  }
}