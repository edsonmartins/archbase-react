{
  "name": "@archbase/security",
  "version": "3.0.12",
  "description": "Security and authentication components for Archbase React",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "NODE_OPTIONS=\"--max-old-space-size=8192\" vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.{ts,tsx}",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "typescript",
    "security",
    "authentication",
    "archbase"
  ],
  "author": "Edson Martins",
  "license": "MIT",
  "dependencies": {
    "@archbase/components": "workspace:*",
    "@archbase/core": "workspace:*",
    "@archbase/data": "workspace:*",
    "@archbase/layout": "workspace:*",
    "crypto-js": "^4.1.1",
    "i18next": "^25.3.1",
    "inversify": "^6.0.1",
    "inversify-react": "^1.1.0",
    "jwt-decode": "^4.0.0",
    "lodash": "4.17.21",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^15.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/lodash": "^4.14.200",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "typescript": "^5.7.0",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "@mantine/core": "8.3.14",
    "@mantine/dates": "8.3.14",
    "@mantine/hooks": "8.3.14",
    "@mantine/modals": "8.3.14",
    "@mantine/notifications": "8.3.14",
    "@tabler/icons-react": "^3.27.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}
