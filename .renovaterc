{
  "regexManagers": [
    {
      "description": "Update semantic-release version used by npx",
      "fileMatch": ["^\\.github/workflows/[^/]+\\.ya?ml$"],
      "matchStrings": ["\\srun: npx semantic-release@(?<currentValue>.*?)\\s"],
      "datasourceTemplate": "npm",
      "depNameTemplate": "semantic-release"
    }
  ]
}